<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
    <update handle="formkey"/>
    <head>
    	<title>Report Manager</title>
    </head>
    <body>
    	<referenceBlock name="menu">
    	    <action method="setActive">
    	        <argument name="itemId" xsi:type="string">Ecomteck_Bannerslider::bannerslider</argument>
    	    </action>
    	</referenceBlock>
    	<referenceBlock name="page.title">
    	    <action method="setPageTitle">
    	        <argument name="pageTitle" xsi:type="string">Report Clicks and Impressions of a Banner in each Slider</argument>
    	    </action>
    	</referenceBlock>
        <referenceContainer name="content">
            <block class="Ecomteck\Bannerslider\Block\Adminhtml\Report" name="admin.block.report.grid.container"/>
        </referenceContainer>
    </body>
</page>
